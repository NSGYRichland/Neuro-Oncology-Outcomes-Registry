@{
    ViewBag.Title = "Manage Data";
}

<div class="jumbotron">
    <h1>Tumor App - Manage</h1>
    <p class="lead">Use the links below to manage possible entries</p>
</div>

<div class="row marketing">
    <div class="col-md-6">
        <h2>Manage Data</h2>

        <div class="col-lg-5">
            <p>
                
                @Html.ActionLink("Possible Health Factors", "Index", "PossibleHealthFactors", null, new { @class = "btn btn-primary btn-large" }) <br />
